{
  "name": "leaflet-schematic",
  "version": "1.0.0-rc.8",
  "description": "Leaflet SVG viewer for non-cartographic high-detailed schematics",
  "main": "index.js",
  "scripts": {
    "test": "browserify -v -t [ babelify --presets [ es2015 ] ] test/*.test.js | tape-run --render='tap-spec'",
    "test:watch": "nodemon --exec 'npm run test'",
    "start": "budo demo/js/index.js --serve=demo/js/build.js --live -d demo",
    "lint": "eslint ./src/",
    "build-js": "browserify -v -t [ babelify --presets [ es2015 ] ] -s L.Schematic -u leaflet ./index.js -o dist/L.Schematic.js",
    "compress": "uglifyjs dist/L.Schematic.js -o dist/L.Schematic.min.js -m --comments",
    "build": "npm run lint && npm run build-js && npm run compress"
  },
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "dependencies": {
    "Base64": "^0.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-filesaver": "^1.1.1",
    "browserify": "^13.0.0",
    "budo": "^9.0.0",
    "eslint": "^2.8.0",
    "faucet": "0.0.1",
    "leaflet": "rc",
    "leaflet-editable": "^1.0.0-rc.1",
    "nodemon": "^1.10.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tape-run": "^2.1.4",
    "uglifyjs": "^2.4.10",
    "xhr": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/w8r/leaflet-schematic.git"
  }
}
